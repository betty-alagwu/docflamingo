{
  "name": "nextjs-clerk-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:ci": "NODE_OPTIONS=\"--max_old_space_size=4096\" NEXT_CONFIG_FILE=next.config.ci.js next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "prettier:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format": "npm run prettier",
    "format:check": "npm run prettier:check",
    "fix": "npm run lint:fix && npm run prettier:fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "check-licenses": "license-checker --production --onlyAllow \"MIT;ISC;Apache-2.0;BSD-2-Clause;BSD-3-Clause;CC0-1.0;0BSD;Unlicense\" --excludePrivatePackages",
    "prepare": "husky"
  },
  "dependencies": {
    "@ai-sdk/deepseek": "^0.1.12",
    "@ai-sdk/openai": "^1.1.15",
    "@ai-sdk/react": "^1.1.18",
    "@clerk/nextjs": "6.9.12",
    "@prisma/client": "^6.3.1",
    "@trigger.dev/sdk": "3.3.17",
    "@types/node": "^22.10.7",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "ai": "^4.1.46",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.9",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "gpt-tokenizer": "^2.9.0",
    "mongodb": "^6.16.0",
    "next": "15.1.4",
    "octokit": "^4.1.2",
    "postcss": "^8.5.1",
    "prism-react-renderer": "^2.4.1",
    "prisma": "^6.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-syntax-highlighter": "^15.6.1",
    "reflect-metadata": "^0.2.2",
    "tailwindcss": "^3.4.17",
    "typedi": "^0.10.0",
    "typescript": "^5.7.3",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.3.2",
    "@trigger.dev/build": "3.3.17",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.3.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "license-checker": "^25.0.1",
    "lint-staged": "^16.0.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.1.2",
    "tsx": "^4.19.2"
  }
}
